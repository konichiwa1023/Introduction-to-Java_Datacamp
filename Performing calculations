class BasicMaths {
    public static void main(String[] args) {
    
    	// Save the result of 5 plus 6 to varA
        int varA = 5 + 6;
        
        // varB is varA minus 3.5
        double varB = varA - 3.5;
        
        // varC is 3 times 3
        int varC = 3 * 3;  
        
        // varD1 is varC divided by two, as int
        int varD1 = varC / 2;
        
        // varD2 is varC divided by two, as double
        double varD2 = varC / 2.0;  
        
        System.out.println("int varD1: " + varD1);
        System.out.println("double varD2: " + varD2);
	}
}
